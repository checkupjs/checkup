import { BaseTask, Category, Priority } from '@checkup/core';
import <%- taskResultClass %> from '../results/<%- name %>-task-result';

export default class <%- taskClass %> extends BaseTask {
  meta = {
    taskName: '<%- name %>',
    friendlyTaskName: '<%- _.startCase(name.split('-').join(' ')) %>',
    taskClassification: {
      category: Category.<%- _.upperFirst(category) %>,
      priority: Priority.<%- _.upperFirst(priority) %>,
    },
  };

  async run() {
    let result = new <%- taskResultClass %>(this.meta);

    return result;
  }
}