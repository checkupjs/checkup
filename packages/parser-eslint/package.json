{
  "name": "@checkup/parser-eslint",
  "description": "ESLint parser for @checkup/cli",
  "version": "0.0.0",
  "author": "Steve Calvert <steve.calvert@gmail.com>",
  "bugs": "https://github.com/checkupjs/checkup/issues",
  "dependencies": {
    "@checkup/core": "0.0.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "eslint": "^6.8.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "ts-node": "^8",
    "type-fest": "^0.12.0",
    "typescript": "^3.8"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/checkupjs/checkup",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "register-parsers": "./lib/hooks/register-parsers"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/checkupjs/checkup",
  "scripts": {
    "build": "tsc",
    "build:watch": "yarn build -w",
    "clean": "rm -rf lib/*",
    "test": "jest --passWithNoTests"
  },
  "types": "lib/index.d.ts"
}
