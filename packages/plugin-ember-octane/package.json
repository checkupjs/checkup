{
  "name": "@checkup/plugin-ember-octane",
  "description": "An oclif plugin for Ember Octane migration tasks",
  "version": "0.0.0",
  "author": "Lewis Miller @lbdm44",
  "bugs": "https://github.com/checkupjs/checkup/issues",
  "dependencies": {
    "@checkup/core": "0.0.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "debug": "^4.1.1",
    "ember-template-lint": "^2.4.1",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^7.11.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@types/eslint": "^6.1.8",
    "babel-eslint": "^10.1.0",
    "fixturify-project": "^2.1.0",
    "jest": "^25.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8",
    "typescript": "^3.8"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/checkupjs/checkup",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "register-tasks": "./src/hooks/register-tasks"
    }
  },
  "repository": "https://github.com/checkupjs/checkup",
  "scripts": {
    "build": "tsc",
    "build:watch": "yarn build -w",
    "clean": "rm -rf lib/*",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "test:debug": "yarn clean && yarn build && node --inspect-brk=1337 node_modules/.bin/jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "main": "lib/index.js"
}
