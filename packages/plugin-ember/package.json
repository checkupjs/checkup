{
  "name": "@checkup/plugin-ember",
  "description": "An oclif plugin for Ember project tasks",
  "version": "0.0.6",
  "author": "Steve Calvert @scalvert",
  "bugs": "https://github.com/checkupjs/checkup/issues",
  "dependencies": {
    "@checkup/core": "0.0.6",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@checkup/test-helpers": "0.0.6",
    "@oclif/dev-cli": "^1",
    "fixturify-project": "^2.1.0",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/checkupjs/checkup",
  "keywords": [
    "checkup-plugin",
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "register-tasks": "./lib/hooks/register-tasks"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/checkupjs/checkup",
  "scripts": {
    "build": "tsc --build",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "tsc --build && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "main": "lib/index.js"
}
