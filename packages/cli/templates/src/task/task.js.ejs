import { BaseTask, Category, Priority } from '@checkup/core';
<%_ const taskClass = `${_.upperFirst(_.camelCase(name))}Task` _%>
<%_ const resultClass = `${taskClass}Result` _%>
import <%- resultClass %> from '../results/<%- name %>-task-result';

export default class <%- taskClass %> extends BaseTask {
  meta = {
    taskName: '<%- name %>',
    friendlyTaskName: '<%- _.startCase(name.split('-').join(' ')) %>',
    taskClassification: {
      category: Category.<%- _.upperFirst(category) %>,
      priority: Priority.<%- _.upperFirst(priority) %>,
    },
  };

  async run() {
    let result = new <%- resultClass %>(this.meta);

    return result;
  }
}
