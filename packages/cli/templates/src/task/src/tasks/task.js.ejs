const { BaseTask } = require('@checkup/core');

module.exports = class <%- taskClass %> extends BaseTask {
  taskName = '<%- name %>';
  taskDisplayName = '<%- _.startCase(name.split('-').join(' ')) %>';
  category = '<%- category %>';
  <%_ if (group) { _%>
  group = '<%- group %>';
  <%_ } _%>


  async run() {
    return [
      {
        message: { text: `${this.taskName} result` },
        occurrenceCount: 1,
        ruleId: this.taskName,
        properties: {
          taskDisplayName: this.taskDisplayName,
          category: this.category,
        },
      },
    ];
  }
}
