{
  "name": "@checkup/cli",
  "description": "A CLI that provides health check information about your project.",
  "version": "0.0.6",
  "author": "Steve Calvert <steve.calvert@gmail.com>",
  "bin": {
    "checkup": "./bin/run"
  },
  "bugs": "https://github.com/checkupjs/checkup/issues",
  "dependencies": {
    "@babel/traverse": "^7.9.5",
    "@babel/types": "7.9.5",
    "@checkup/core": "0.0.6",
    "@checkup/plugin-ember": "0.0.6",
    "@checkup/plugin-ember-octane": "0.0.6",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^4.0.0",
    "chrome-debugging-client": "^1.2.0",
    "date-and-time": "^0.13.1",
    "debug": "^4.1.1",
    "devtools-protocol": "^0.0.757450",
    "fs-extra": "^9.0.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.15",
    "p-map": "^4.0.0",
    "promise.hash.helper": "^1.0.7",
    "recast": "^0.19.0",
    "shorthash": "^0.0.2",
    "tmp": "^0.1.0",
    "tslib": "^1",
    "yeoman-environment": "^2.9.1",
    "yeoman-generator": "^4.8.0"
  },
  "devDependencies": {
    "@checkup/test-helpers": "0.0.6",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/yeoman-environment": "^2.3.3",
    "@types/yeoman-generator": "^3.1.4",
    "yeoman-test": "^2.4.1"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/checkupjs/checkup",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "checkup"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/checkupjs/checkup",
  "scripts": {
    "build": "yarn clean && tsc && yarn copystatic",
    "build:watch": "yarn build -w",
    "clean": "rm -rf lib",
    "prepack": "yarn build && oclif-dev readme",
    "test": "jest --runInBand --no-cache",
    "version": "oclif-dev readme && git add README.md",
    "copystatic": "cp -a ./src/static ./lib/static"
  },
  "types": "lib/index.d.ts"
}
