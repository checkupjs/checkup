import { BaseTask, TaskType, Task, TaskMetaData, TaskResult } from '@checkup/core';
import <%- taskResultClass %> from '../results/<%- name %>-task-result';

export default class <%- taskClass %> extends BaseTask implements Task {
  meta: TaskMetaData = {
    taskName: '<%- name %>',
    friendlyTaskName: '<%- _.startCase(name.split('-').join(' ')) %>',
    taskClassification: {
      type: TaskType.<%- _.upperFirst(type) %>,
      category: '<%- category %>',
    },
  };

  async run(): Promise<TaskResult> {
    let result: <%- taskResultClass %> = new <%- taskResultClass %>(this.meta);

    return result;
  }
}
